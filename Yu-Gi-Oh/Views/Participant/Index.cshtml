@model IEnumerable<Yu_Gi_Oh.Models.Participant>

@{
    ViewBag.Title = "Participantes";
}

<h2>Participantes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParticipantNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlayerNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContestName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeckName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
            <tr>
               <td>
                  @Html.DisplayFor(modelItem => item.ParticipantNumber)
               </td>
               <td>
                  @Html.DisplayFor(modelItem => item.PlayerNumber)
               </td>
               <td>
                  @Html.DisplayFor(modelItem => item.ContestName)
               </td>
               <td>
                  @Html.DisplayFor(modelItem => item.DeckName)
               </td>
           </tr>
    }
</table>